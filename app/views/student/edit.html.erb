<h1>Edit user</h1>

<%= form_for(@user) do |f| %>
<%= render 'shared/error_messages', :object => f.object %> <div class="field">
<%= f.label :first_name %><br />
<%= f.text_field :first_name %> </div>
<div class="field">
<%= f.label :last_name %><br /> <%= f.text_field :last_name %>
</div>
<div class="field">
<%= f.label :id %><br />
<%= f.password_field :id %> </div>
<div class="actions">
<%= f.submit "Update" %> </div>
<% end %>
